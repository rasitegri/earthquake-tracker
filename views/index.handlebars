<div class="row">
    <div class="col">
        <h1>{{title}}</h1>
        <table class="table table-dark">
            <thead>
            <tr>
                <th scope="col">Tarih</th>
                <th scope="col">Saat</th>
                <th scope="col">Büyüklük</th>
                <th scope="col">Yer</th>
            </tr>
            </thead>
            <tbody>
                {{#each earthquakeList}}
                <tr>
                    <th scope="row">{{this.date}}</th>
                    <td>{{this.time}}</td>
                    <td>{{floatToFixed this.ML}}</td>
                    <td>{{this.yer}}</td>
                </tr>
                {{/each}}
            </tbody>
        </table>
    </div>
</div>
